    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabLayoutActivity" />
        <activity android:name=".ViewDragHelperActivity" />
        <activity android:name=".VideoActivity" />
        <activity
            android:name=".zhiHuActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name=".SlideCutActivity" />
        <activity android:name=".ViewStyleActivity" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".AsyncTaskActivity" />
        <activity android:name=".AnnotationActivity" />
        <activity android:name=".HorizontalListActivity" />
        <activity android:name=".HttpActivity" />
        <activity android:name=".ListPackageInfoActivity" />
        <activity android:name="com.txt.wxmvp.WXActivity" />
        <activity android:name=".ViewPagerGalleryActivity" />
        <activity android:name=".Service1Activity" />
        <activity android:name=".Service2Activity" />
        <activity android:name=".AidlActivity" />
        <activity android:name=".AidlBinderPoolActivity" />
        <activity android:name=".MessengerActivity" />
        <activity android:name=".FragmentActivity" />
        <activity android:name=".MyViewPagerActivity" />
        <activity android:name=".RecycleViewActivity" />
        <activity android:name=".CustomViewActivity" />
        <activity android:name=".StickyHeaderListActivity" />
        <activity android:name=".DexPathTestActivity" />
        <activity android:name=".LifecycleActivity" />
        <activity android:name=".LifecycleTargetActivity1" />
        <activity
            android:name=".LifecycleTargetActivity2"
            android:theme="@android:style/Theme.Translucent" />
        <activity
            android:name=".LifecycleTargetActivity3"
            android:theme="@style/Theme.AppCompat.Dialog" />

        <service android:name="com.txt.service.MyIntentService" />
        <service android:name="com.txt.service.MyService" />
        <service
            android:name="com.txt.service.MyAidlService"
            android:process=":remote" />
        <service
            android:name="com.txt.service.MyAidlBinderPoolService"
            android:process=":remote" />
        <service
            android:name="com.txt.service.MyMessengerService"
            android:process=":remote" />

        <activity android:name="com.txt.web2native.Web2nativeActivity" />
        <activity
            android:name="com.txt.recycleview.RecycleViewHeaderActivity"
            android:label="@string/title_activity_recycle_view_header"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.txt.threadtoast.ThreadTostActivity"
            android:label="@string/title_activity_thread_tost"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.txt.plugin.ProxyActivity">
            <intent-filter>
                <action android:name="com.ryg.dynamicloadhost.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tongxt.aidlservicedemo.AidlActivity"
            android:label="@string/title_activity_aidl"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.txt.scrollnumber.ScrollNumberActivity" />

        <service
            android:name="com.txt.service.ForegroundService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".KeepLiveActivity" />

        <receiver android:name=".MainActivity$KeepLiveReceiver" />

        <activity
